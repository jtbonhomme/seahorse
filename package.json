{
  "name": "seahorse",
  "description": "Configurable mock REST API testing tool written in javascript ",
  "repository": {
    "type": "git",
    "url": "https://github.com/jtbonhomme/seahorse.git"
  },
  "version": "0.2.6",
  "author": "jtbonhomme@gmail.com",
  "main": "./lib/main.js",
  "bin": "./bin/seahorse.js",
  "scripts": {
    "start": "node ./bin/seahorse.js -f example/config.json -t",
    "start80": "sudo node ./bin/seahorse.js -f example/config.json -t -p 80"
