    "throttle": "git+https://github.com/jtbonhomme/node-throttle.git"
  },
  "devDependencies": {
    "grunt-concurrent": "~0.5.0",
    "grunt-contrib-concat": "~0.5.0",
    "grunt-contrib-uglify": "~0.5.1",
    "grunt-mocha-test": "^0.11.0",
    "grunt-nodemon": "~0.3.0",
    "hippie": "~0.3.0",
    "install": "^0.1.7",
    "npm": "^1.4.23",
    "should": "^4.0.4"
  }
}
